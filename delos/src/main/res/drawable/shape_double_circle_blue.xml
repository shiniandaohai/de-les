<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <corners
                android:radius="20dp"/>
            <stroke android:width="1dp" android:color="#128ce3"/>
            <size android:width="10dp"
                android:height="10dp"/>
        </shape>
    </item>
    <item android:left="2dp"
          android:top="2dp"
          android:right="2dp"
          android:bottom="2dp">
        <shape>
            <solid android:color="#128ce3"/>
            <corners
                android:radius="20dp"/>
            <size android:width="10dp"
                  android:height="10dp"/>
        </shape>
    </item>
</layer-list>
